int main () {
  int x = 0;
  int y = 0;
  int z = 0;

  while (z < 42) {
    if (z < 28) {
        x++;
        if (z < 14) y++;
    }
    z++;
  }
  # x == 28 && y == 14
  if (x == y * 2 && x + y == 42) {
    printString("success!");
    # prints success!
  } else {
    printString("FAIL!");
  }
  return 0;
}
